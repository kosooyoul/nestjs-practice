{
  "name": "practice-nestjs",
  "version": "0.0.2",
  "description": "",
  "author": "kosooyoul",
  "private": true,
  "license": "AGPL-3.0",
  "scripts": {
    "prebuild": "rimraf dist",
    "build": "nest build",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "generate-sdl": "nest start --entryFile ./scripts/sdl-generator",
    "start": "nest start",
    "start:dev": "nest start --watch",
    "start:debug": "nest start --debug --watch",
    "start:prod": "node dist/main",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "test": "jest --verbose --runInBand --detectOpenHandles --forceExit",
    "test:watch": "jest --watch --runInBand",
    "test:cov": "jest --coverage --runInBand",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "jest --config ./test/jest-e2e.json",
    "prisma:generate": "prisma generate --schema prisma/hanulse-schema.prisma",
    "prisma:push": "prisma db push --schema prisma/hanulse-schema.prisma",
    "prisma:pull": "prisma db pull --schema prisma/hanulse-schema.prisma",
    "prisma:studio": "prisma studio --schema prisma/hanulse-schema.prisma"
  },
  "dependencies": {
    "@nestjs/apollo": "^10.0.11",
    "@nestjs/common": "^8.0.0",
    "@nestjs/config": "^2.0.1",
    "@nestjs/core": "^8.0.0",
    "@nestjs/event-emitter": "^1.3.1",
    "@nestjs/graphql": "^10.0.11",
    "@nestjs/jwt": "^8.0.1",
    "@nestjs/passport": "^8.2.1",
    "@nestjs/platform-express": "^8.0.0",
    "@nestjs/platform-socket.io": "^8.4.5",
    "@nestjs/swagger": "^5.2.1",
    "@nestjs/websockets": "^8.4.5",
    "@prisma/client": "^4.11.0",
    "@socket.io/redis-adapter": "^7.1.0",
    "@typegoose/typegoose": "^9.8.1",
    "aes256": "^1.1.0",
    "apollo-server-core": "^3.7.0",
    "apollo-server-express": "^3.7.0",
    "aws-sdk": "^2.1082.0",
    "axios": "^0.27.2",
    "bcrypt": "^5.0.0",
    "cache-manager": "^4.0.1",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.13.2",
    "graphql": "^16.5.0",
    "graphql-type-json": "^0.3.2",
    "graphql-upload": "^13.0.0",
    "hbs": "^4.2.0",
    "lodash": "^4.17.21",
    "md5": "^2.3.0",
    "moment": "^2.29.3",
    "moment-timezone": "^0.5.34",
    "mongoose": "^6.3.3",
    "nestjs-typegoose": "^7.1.38",
    "passport": "^0.5.3",
    "passport-jwt": "^4.0.0",
    "proj4": "^2.7.4",
    "reflect-metadata": "^0.1.13",
    "request": "^2.88.2",
    "rimraf": "^3.0.2",
    "rxjs": "^7.2.0",
    "socket.io": "^4.5.1",
    "swagger-ui-express": "^4.4.0"
  },
  "devDependencies": {
    "@nestjs/cli": "^8.0.0",
    "@nestjs/schematics": "^8.0.0",
    "@nestjs/testing": "^8.0.0",
    "@types/cache-manager": "^4.0.0",
    "@types/express": "^4.17.13",
    "@types/graphql-upload": "^8.0.11",
    "@types/jest": "27.5.0",
    "@types/node": "^16.0.0",
    "@types/passport-jwt": "^3.0.6",
    "@types/socket.io-redis": "^3.0.0",
    "@types/supertest": "^2.0.11",
    "@typescript-eslint/eslint-plugin": "^5.0.0",
    "@typescript-eslint/parser": "^5.0.0",
    "eslint": "^8.0.1",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-prettier": "^4.0.0",
    "jest": "28.0.3",
    "prettier": "^2.3.2",
    "prisma": "^4.11.0",
    "supertest": "^6.1.3",
    "ts-jest": "28.0.1",
    "ts-loader": "^9.2.3",
    "ts-node": "^10.0.0",
    "tsconfig-paths": "4.0.0",
    "typescript": "^4.3.5",
    "uuid": "^8.3.2",
    "uuid-base64": "^1.0.0"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": "src",
    "testRegex": ".*\\.spec\\.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "collectCoverageFrom": [
      "**/*.(t|j)s"
    ],
    "coverageDirectory": "../coverage",
    "testEnvironment": "node",
    "moduleNameMapper": {
      "^@/(.*)$": "<rootDir>/src/$1"
    }
  }
}
